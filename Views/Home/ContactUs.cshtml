<div class="content-page contactus">
    <div class="content-text">
        <img class="contactus-photo" src="/img/contact-us.png" alt="contact us"/>
        <h4>You may contact us via the following:</h4><br />
        <h4>Telephone</h4>
        <p>@System.Configuration.ConfigurationManager.AppSettings["phoneNumber"]</p>
        <h4>Address</h4>
        <p>@System.Configuration.ConfigurationManager.AppSettings["address"]</p>
        <h4>Email</h4>
        <p><a href="mailto:@System.Configuration.ConfigurationManager.AppSettings["contactEmail"]">@System.Configuration.ConfigurationManager.AppSettings["contactEmail"]</a></p>
    </div>
</div>